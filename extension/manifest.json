{
    "name": "Sintco Sparxmaths Solver",
    "version": "3.0.1",
    "description": "Easy Bookwork-code Bypass",
    "manifest_version": 3,
    "author": "Najm Ajmal",
    "host_permissions": [
        "*://www.sparxmaths.uk/*",
        "https://auth.sparxmaths.uk/*"
    ],
    "icons": {
        "256": "assets/logo.png"
    },
    "action": {
        "default_icon": {
            "256": "assets/logo.png"
        },
        "default_title": "Sintco Sparxmaths Solver"
    },
    "permissions": [
        "activeTab",
        "storage",
        "downloads"
    ],
    "content_scripts": [
        {
            "js": [
                "inject.js"
            ],
            "css": [
                "styles.css"
            ],
            "matches": [
                "https://*.sparxmaths.uk/*"
            ],
            "run_at": "document_start"
        }
    ],
    "web_accessible_resources": [
        {
            "resources": [
                "bundle.js","inject.js","https://raw.githubusercontent.com/SintcoLTD/CDN/main/chrome-extension/sparxmaths/release.json"
            ],
            "matches": [
                "<all_urls>"
            ]
        }
    ]
}
